{
  "name": "action-turbo-changed",
  "version": "1.0.0",
  "private": true,
  "description": "A Github Action making it easy to check if a local workspace changed using Turborepo.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Trampoline-CX/action-turbo-changed.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Trampoline",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/github": "5.1.1"
  },
  "devDependencies": {
    "@trampoline/eslint-config": "1.4.0",
    "@types/node": "18.11.10",
    "@vercel/ncc": "0.34.0",
    "eslint": "^8.29.0",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.8.0",
    "ts-jest": "^27.1.2",
    "typescript": "4.9.3"
  },
  "packageManager": "yarn@3.3.0"
}
