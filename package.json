{
  "name": "action-turbo-changed",
  "version": "2.0.1",
  "private": true,
  "description": "A Github Action making it easy to check if a local workspace changed using Turborepo.",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write '**/*.ts'",
    "format-check": "prettier --check '**/*.ts'",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "yarn build && yarn format && yarn lint && yarn package && yarn test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Trampoline-CX/action-turbo-changed.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "Trampoline",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "3.0.0",
    "@npmcli/package-json": "7.0.4",
    "@types/npmcli__package-json": "4.0.4",
    "semver": "7.7.4"
  },
  "devDependencies": {
    "@trampoline/eslint-config": "1.8.0",
    "@types/node": "24.10.2",
    "@vercel/ncc": "0.38.4",
    "eslint": "8.57.1",
    "jest": "^30.2.0",
    "js-yaml": "^4.1.1",
    "prettier": "3.7.4",
    "ts-jest": "^29.4.6",
    "typescript": "5.9.3"
  },
  "packageManager": "yarn@4.5.0"
}
